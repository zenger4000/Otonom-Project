@model FinalProject.ViewModels.Teams

@{
    ViewData["Title"] = "Home";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<br />
<br />
<br />
<div style="margin: 20px 330px">
    <h1>Create</h1>

    <h4>Teams</h4>
    <hr />
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Create" enctype="multipart/form-data">
                <div asp-validation-summary="All" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="Name" class="control-label"></label>
                    <input asp-for="Name" class="form-control" />
                    <span asp-validation-for="Name" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="description" class="control-label"></label>
                    <input asp-for="description" class="form-control" />
                    <span asp-validation-for="description" class="text-danger"></span>
                </div>
                <div class="form-control">
                    <label asp-for="Picture" class="control-label"></label>
                    <input type="file" asp-for="Picture" class="form-control" />
                    <span asp-validation-for="Picture" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Facebook" class="control-label"></label>
                    <input asp-for="Facebook" class="form-control" />
                    <span asp-validation-for="Facebook" class="text-danger"></span>
                </div>

                <div class="form-group">
                    <label asp-for="Linkdin" class="control-label"></label>
                    <input asp-for="Linkdin" class="form-control" />
                    <span asp-validation-for="Linkdin" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="position" class="control-label"></label>
                    <input asp-for="position" class="form-control" />
                    <span asp-validation-for="position" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>

    
    </div>
    @section Scripts {
            @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
        }
